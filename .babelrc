{
  "presets": ["next/babel", "airbnb"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "constants": "./constants",
          "pages": "./pages",
          "static": "./static",
          "utils": "./utils",
          "underscore": "lodash"
        }
      }
    ]
  ]
}
